drop table if exists autos;

create table autos(
codigo int unsigned auto_increment,
titulo varchar(30) not null,
actor varchar(20),
duracion int,
primary key (codigo)
);

 insert into autos (titulo,actor,duracion) values ('Mision imposible','Tom Cruise',120);
 /*insert into autos (codigo,duracion) values (5,90);*/
 /*insert into autos (titulo,actor) values ('Harry Potter y la piedra filosofal','Daniel R.');*/
 /*insert into autos (titulo,actor,duracion) values ('Harry Potter y la piedra filosofal','Daniel R.',120);*/
 
 select * from autos;
